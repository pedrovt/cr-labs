<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="CustomPeriphDemo"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
CustomPeriphDemo
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="CustomPeriphDemo">
        <sdx:configuration sdx:name="CustomPeriphDemo"
                           sdx:displayName="CustomPeriphDemo"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>CustomPeriphDemo</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="CustomPeriphDemo/boot/CustomPeriphDemo.bif"
                            sdx:readme="CustomPeriphDemo/boot/generic.readme"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on microblaze_0"
                                sdx:cpuType="microblaze_0"
                                sdx:cpuInstance="microblaze_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="CustomPeriphDemo/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="CustomPeriphDemo/standalone_domain/bspinclude/include"
                        sdx:bspConfig="CustomPeriphDemo/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
